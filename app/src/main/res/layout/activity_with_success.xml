<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/bgm"
    android:layout_gravity="center_horizontal"
    android:gravity="center_horizontal"
    tools:context="com.neishenmo.sochat.sochatandroid.view.personage.WithSuccessActivity">
<ImageView
    android:layout_width="120dp"
    android:layout_height="120dp"
    android:background="@drawable/succes"
    android:layout_marginTop="100dp"
    />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#ffffff"
        android:text="操作成功"
        android:textSize="32sp"
        android:layout_marginTop="10dp"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="提现金额将于两小时左右到指定账户"
        android:textColor="#D1D7FC"
        android:layout_marginTop="10dp"
        />
    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/ok_ok"
        android:background="@drawable/ok_ok"
        android:layout_marginTop="25dp"
        />
</LinearLayout>
